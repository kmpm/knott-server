extends ../layout

block content
  .tabbable.tabs-left
    ul.nav.nav-tabs  
      for categories, driver in config
        li
          a(href="##{driver}", data-toggle="tab")=driver

    .tab-content
      for categories, driver in config
        .tab-pane(id="#{driver}")
          include slask 
          
        
block scripts
  script(src='/socket.io/socket.io.js')
  script(src="/javascripts/config.js")
  script
    $(document).ready(function(){
      deviceConfig();
      listenSocket();
    });

